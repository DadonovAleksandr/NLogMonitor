{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "App": {
    "Mode": "Desktop"
  },
  "SessionSettings": {
    "FallbackTtlMinutes": 5,
    "CleanupIntervalMinutes": 1
  },
  "FileSettings": {
    "MaxFileSizeMB": 100,
    "AllowedExtensions": [".log", ".txt"],
    "TempDirectory": "./temp"
  },
  "RecentLogsSettings": {
    "MaxEntries": 20,
    "CustomStoragePath": null
  }
}
